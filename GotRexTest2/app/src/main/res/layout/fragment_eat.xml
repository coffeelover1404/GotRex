<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EatFragment">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:id="@+id/EatLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="314dp"
            android:contentDescription="@string/Baby"
            android:src="@mipmap/ic_launcher_foreground" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="162dp"
                android:layout_height="118dp"
                android:layout_weight="1"
                android:contentDescription="@string/Food1"
                android:src="@drawable/food_004_o" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="184dp"
                android:layout_height="118dp"
                android:layout_weight="1"
                android:contentDescription="@string/Food2"
                android:src="@drawable/junkfood" />
        </LinearLayout>
    </LinearLayout>

</FrameLayout>